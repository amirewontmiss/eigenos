{
  "name": "quantumos",
  "version": "1.0.0",
  "description": "Enterprise-Grade Quantum Operating System",
  "main": "dist/main.js",
  "author": "QuantumOS Team",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc && electron-builder",
    "build:main": "tsc",
    "build:renderer": "echo 'Renderer build placeholder - implement React app build'",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"wait-on dist/main.js && electron .\"",
    "dev:main": "tsc -w",
    "dev:renderer": "echo 'Renderer dev server placeholder - implement React dev server'",
    "start": "electron .",
    "start:prod": "npm run build && electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "prepare": "npm run build:main",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "mongoose": "^8.0.0",
    "nestjs": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.0",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0",
    "zustand": "^4.4.0",
    "typescript-collections": "^1.3.3"
  }
}