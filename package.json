{
  "name": "quantumos",
  "version": "1.0.0",
  "description": "Enterprise-Grade Quantum Operating System",
  "main": "dist/main.js",
  "author": "QuantumOS Team",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc && electron-builder",
    "build:main": "tsc",
    "build:renderer": "webpack --mode production --config webpack.renderer.config.js",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"wait-on dist/main.js && wait-on http://localhost:3000 && electron .\"",
    "dev:main": "tsc -w",
    "dev:renderer": "webpack serve --mode development --config webpack.renderer.config.js",
    "start": "electron .",
    "start:prod": "npm run build && NODE_ENV=production electron .",
    "start:dev": "NODE_ENV=development npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "prepare": "npm run build:main",
    "postinstall": "electron-builder install-app-deps",
    "env:setup": "cp .env.example .env",
    "env:validate": "node -e \"require('./dist/config/environment.config.js').validateConfig()\"",
    "python:setup": "python3 -m venv quantum-env && source quantum-env/bin/activate && pip install -r requirements.txt",
    "python:install": "pip install -r requirements.txt",
    "db:migrate": "typeorm migration:run",
    "db:revert": "typeorm migration:revert",
    "healthcheck": "node -e \"require('./dist/app.service.js').appService.healthCheck().then(console.log)\"",
    "maintenance": "node -e \"require('./dist/app.service.js').appService.performMaintenance().then(() => console.log('Maintenance completed'))\""
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.50.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.0",
    "postcss-loader": "^7.3.0",
    "rimraf": "^5.0.0",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "mongoose": "^8.0.0",
    "nestjs": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.0",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0",
    "zustand": "^4.4.0",
    "typescript-collections": "^1.3.3"
  }
}